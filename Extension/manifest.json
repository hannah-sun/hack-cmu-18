 {
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage"],
    "background": {
	    "scripts": ["background.js"]
    },
    "browser_action": {
	    "default_popup": "popup.html"
    },
    "content_scripts":[
      {
       "matches": [
       "<all_urls>"
        ],
        "js": ["content.js"]
      }
     ],
    "permissions" : ["notifications", "storage"],
    "manifest_version": 2
  }
